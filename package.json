{
  "name": "react-express-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch:client": "parcel watch client/index.html --dist-dir=dist/client",
    "watch:server": "tsc-watch --project server --onSuccess \"node dist/server/index.js\"",
    "dev": "concurrently \"npm run watch:client\" \"npm run watch:server\"",
    "lint": "eslint \"client/**/*.{js,jsx,ts,tsx}\" \"server/**/*.{js,jsx,ts,tsx}\" --quiet",
    "format": "prettier --write \"server/**/*.{js,jsx,ts,tsx}\" \"client/**/*.{js,jsx,ts,tsx}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.3",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
